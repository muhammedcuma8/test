{
  "name": "{DOCKER_WORKDIR}",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "vite",
    "build": "vite build",
    "start:dev": "env-cmd -f ./env/.env.development vite",
    "test": "vite test",
    "build:dev": "env-cmd -f ./env/.env.development vite build",
    "build:test": "env-cmd -f ./env/.env.test vite build",
    "build:prod": "env-cmd -f ./env/.env.production vite build",
    "build:scss": "sass --no-source-map public/assets/layout/css/layout-light.scss public/assets/layout/css/layout-light.css && sass --no-source-map public/assets/theme/indigo/theme-light.scss public/assets/theme/indigo/theme-light.css && sass --no-source-map public/assets/layout/css/layout-dark.scss public/assets/layout/css/layout-dark.css && sass --no-source-map public/assets/theme/indigo/theme-dark.scss public/assets/theme/indigo/theme-dark.css",
    "build:Watchscss": "sass --watch --no-source-map public/assets/layout/css/layout-light.scss public/assets/layout/css/layout-light.css && sass --watch --no-source-map public/assets/theme/indigo/theme-light.scss public/assets/theme/indigo/theme-light.css",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "release": "standard-version"
  },
  "dependencies": {
    "@types/node": "^20.3.1",
